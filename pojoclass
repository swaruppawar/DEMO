package democode;

import java.util.TreeSet;

	public class pojoclass implements Comparable <pojoclass>{
		private int Id;
		private String name;
		private String comapny;
		

		public int compareTo(pojoclass o) {
			return o.Id-this.Id; //this.id-o.id;
		}

	public pojoclass(int id, String name, String comapny) {
			super();
			Id = id;
			this.name = name;
			this.comapny = comapny;
		}

	public int getId() {
			return Id;
		}

		public String getName() {
			return name;
		}

		public String getComapny() {
			return comapny;
		}

	@Override
		public String toString() {
			return "\n"+"ID" + Id + ", name=" + name + ", comapny=" + comapny ;
		}

	public static void main(String[] args) {
		TreeSet ob=new TreeSet();
		

		ob.add(new pojoclass (51,"manthan","IBM"));
		ob.add(new pojoclass (33,"Raj","tcs"));
		ob.add(new pojoclass (67,"NItesh","infosys"));
		ob.add(new pojoclass (21,"Sagar","fIS"));
		
		System.out.println(ob);

	}

}
