import java.util.*;

public class matrixDaigonal {
	int num[][]= {{1,2,3},{4,5,6},{7,8,9}};
	int sum=0;
	public int go() {
		for (int i = 0; i < num.length; i++) {
			for (int j = 0; j < num.length; j++) {
				if(i==j) {
					sum=sum+num[i][j];
				}
				if(i+j == num.length-1) {
					sum=sum+num[i][j];
				}
			}
			
		}
		return sum;
	}
	public static void main(String[] args) {
		matrixDaigonal obj=new matrixDaigonal();
		int k=obj.go();
		System.out.println(k);
	}
}
